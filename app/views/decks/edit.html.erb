<%#= render :partial => "shared/side_bar" %>
<%= render :partial => "decks/template_selector", :locals => {:template_data => @template_data} %>

<div id='container-custom'>
	<div id='impress' deck_id="<%= @deck.id %>" url='<%= @deck.url %>' class='seven columns' style='z-index:-1'>
		<%# @deck.deck_data.each_with_index do |step, index| %>
			<%#= render :partial => 'step', :locals => {:step => step, :index => index} %>
		<%# end %>
		<% 50.times do |i| %>
			<div class='step editable' data-x='0' data-y='0' data-z='0' data-rotate-x='0' data-rotate-y='0' data-rotate-z='0' data-scale='1'>
			</div>
		<% end %>
	</div>
</div>

<%#= javascript_include_tag "bx_slide" %>
<%#= javascript_include_tag "sideSlideBar" %>
<%= javascript_include_tag "pickle.js" %>
